    "targets": [
      "node18-win-x64",
      "node18-win-arm64"
    ],
    "outputPath": "dist"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "node-printer": "^1.0.4"
  },
  "devDependencies": {
    "pkg": "^5.8.1"
  }
}

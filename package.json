{
  "name": "printer_pos",
  "version": "1.0.0",
  "description": "Middleware printer POS menggunakan Node.js",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node index.js",
    "dev": "nodemon index.js",
    "build-win": "pkg . --targets node18-win-x64,node18-win-arm64 --out-dir dist/windows",
    "build-mac": "pkg . --targets node18-macos-x64,node18-macos-arm64 --out-dir dist/macos",
    "build": "npm run build-win && npm run build-mac"
  },
  "bin": {
    "printer_pos": "./index.js"
  },
  "pkg": {
    "scripts": "index.js",
    "assets": [
      "views/**/*",
      "node_modules/node-printer/printer.js",
      "node_modules/node-printer/job.js"
    ],
    "targets": [
      "node18-win-x64",
      "node18-win-arm64",
      "node18-macos-x64",
      "node18-macos-arm64"
    ],
    "outputPath": "dist"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "node-printer": "^1.0.4"
  },
  "devDependencies": {
    "@yao-pkg/pkg": "^5.15.0"
  }
}

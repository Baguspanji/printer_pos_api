{
  "name": "printer_pos",
  "version": "1.0.0",
  "description": "Middleware printer POS menggunakan Node.js",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node index.js",
    "dev": "nodemon index.js",
    "build-win": "pkg . --targets node18-win-x64,node18-win-arm64",
    "build-mac": "pkg . --targets node18-macos-x64,node18-macos-arm64",
    "build-linux": "pkg . --targets node18-linux-x64"
  },
  "bin": {
    "printer_pos": "./index.js"
  },
  "pkg": {
    "scripts": [
      "index.js",
      "views/printer-status.html"
    ],
    "output": "dist/printer_pos.exe"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "node-printer": "^1.0.4"
  }
}

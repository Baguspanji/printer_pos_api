{
  "name": "printer_pos",
  "version": "1.0.0",
  "description": "Middleware printer POS menggunakan Node.js",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node index.js",
    "dev": "nodemon index.js",
    "build-win": "pkg ."
  },
  "bin": {
    "printer_pos": "./index.js"
  },
